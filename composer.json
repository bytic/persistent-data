{
  "name": "bytic/persistent-data",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/persistent-data",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\PersistentData\\Legacy\\": "legacy",
      "ByTIC\\PersistentData\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\PersistentData\\Tests\\": "tests/src",
      "ByTIC\\PersistentData\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=8.0",
    "bytic/collections": "^1.0",
    "bytic/cookie": "^1.0|^2.0",
    "bytic/orm": "^1.0|^2.0",
    "bytic/session": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/http": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
